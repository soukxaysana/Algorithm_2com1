#include <stdio.h>

int main() {
    int arraysize = 10;
    int arr[arraysize];
    int i, j, key;

    for (i = 0; i < arraysize; i++) {
        printf("Enter value for array %d: ", i+1);
        scanf("%d", &arr[i]);
    }

    // sorting algorithm (insertion sort) 
    for (i = 1; i < arraysize; i++) {
        key = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > key) {
            arr[j+1] = arr[j];
            j--;
        }
        arr[j+1] = key;
    }

    printf("\nSorted array:\n");
    for (i = 0; i < arraysize; i++) {
        printf("%d\n", arr[i]);
    }

    return 0;
}
